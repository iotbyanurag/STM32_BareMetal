#ifndef INC_ST7735_INTERFACES_H_
#define INC_ST7735_INTERFACES_H_


#include "stm32l4xx_hal.h"


#define CS_PIN 		GPIO_PIN_14
#define DC_PIN 		GPIO_PIN_13
#define DIN_PIN		GPIO_PIN_3
#define SCK_PIN 	GPIO_PIN_10
#define RESET_PIN 	GPIO_PIN_6

#define Set_CS_PIN_HIGH() HAL_GPIO_WritePin(GPIOB, GPIO_PIN_14, GPIO_PIN_SET);

#endif /* INC_ST7735_INTERFACES_H_ */
